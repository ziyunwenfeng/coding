<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="springHelloWorld"
        class="com.ta.wenfeng.spring.helloworld.imp.SpringHelloWorld"></bean>
    <bean id="strutsHelloWorld"
        class="com.ta.wenfeng.spring.helloworld.imp.StrutsHelloWorld"></bean>
	<bean id ="helloWorldService" class = "com.ta.wenfeng.spring.helloworld.HelloWorldService" >
		<property name="helloworld" ref = "springHelloWorld"/>
	</bean>
  
</beans>